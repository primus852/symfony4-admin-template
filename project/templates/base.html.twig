<body>

<aside id="sidebar" class="nano">
    <div class="nano-content" id="pScrollerMenu">
        <div class="logo-container"><span class="logo fa fa-code"></span>TITLE</div>
        <div class="mw-version">User: {{ app.user ? app.user.username : 'NONE' }} - v{{ render(controller('App\\Controller\\RenderController::version')) }}</div>
        <div class="menu-segment">
            <div class="row">
                <div class="col-12 text-center">
                    <a href="{{ path('logout') }}" style="color:white;" class="btn btn-success btn-sm rounded-0">Logout <i class="fa fa-lock"></i> </a>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-12">
                    <ul>
                        <li>
                            <div class="title" style="float:left;"><p id="startClock"></p> (Week {{ "now"|date("W") }})</div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <br/>
        <div class="separator"></div>
        <menu class="menu-segment">
            <ul>
                <li id="contentMenu1">
                    <a href="#" class="mLink">
                        <i class="fa fa-list"></i> MENU 1
                    </a>
                </li>
                <ul>
                    <li>
                        <a href="#" class="mLink toggleTree">
                            <i class="fa fa-chevron-right"></i> Tree Menu
                        </a>
                        <ul class="treeMenu">

                            <li id="contentTree1" class="treeItem">
                                <a href="#" class="mLink">
